<?php

/**
 * @file
 * Contains bot4hire.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function bot4hire_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the bot4hire module.
    case 'help.page.bot4hire':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}
